<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<OfficeApp xmlns="http://schemas.microsoft.com/office/appforoffice/1.1" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bt="http://schemas.microsoft.com/office/officeappbasictypes/1.0" xmlns:ov="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="TaskPaneApp">
  <Id>1886423f-dd43-4f4a-a200-e60e1fd6168d</Id>
  <Version>1.0.0.0</Version>
  <ProviderName>maximecharriere</ProviderName>
  <DefaultLocale>en-US</DefaultLocale>
  <DisplayName DefaultValue="WordReqInserter"/>
  <Description DefaultValue="Add-in for inserting unique requirements number in Word."/>
  <IconUrl DefaultValue="https://maximecharriere.github.io/WordReqInserter/assets/icon-32.png"/>
  <HighResolutionIconUrl DefaultValue="https://maximecharriere.github.io/WordReqInserter/assets/icon-64.png"/>
  <SupportUrl DefaultValue="https://maximecharriere.ch/"/>
  <AppDomains>
    <AppDomain>https://maximecharriere.github.io/WordReqInserter/</AppDomain>
  </AppDomains>
  <Hosts>
    <Host Name="Document"/>
  </Hosts>
  <DefaultSettings>
    <SourceLocation DefaultValue="https://maximecharriere.github.io/WordReqInserter/commands.html"/>
  </DefaultSettings>
  <Permissions>ReadWriteDocument</Permissions>
  <VersionOverrides xmlns="http://schemas.microsoft.com/office/taskpaneappversionoverrides" xsi:type="VersionOverridesV1_0">
    <Hosts>
      <Host xsi:type="Document">
        <DesktopFormFactor>
          <GetStarted>
            <Title resid="GetStarted.Title"/>
            <Description resid="GetStarted.Description"/>
            <LearnMoreUrl resid="GetStarted.LearnMoreUrl"/>
          </GetStarted>
          <FunctionFile resid="Commands.Url"/>
          <ExtensionPoint xsi:type="PrimaryCommandSurface">
            <OfficeTab id="TabHome">
              <Group id="ESPIGroup">
                <Label resid="ESPIGroup.Label"/>
                <Icon>
                  <bt:Image size="16" resid="Icon.16x16"/>
                  <bt:Image size="32" resid="Icon.32x32"/>
                  <bt:Image size="80" resid="Icon.80x80"/>
                </Icon>
                <Control xsi:type="Button" id="InsertRequirementButton">
                  <Label resid="InsertRequirementButton.Label"/>
                  <Supertip>
                    <Title resid="InsertRequirementButton.Label"/>
                    <Description resid="InsertRequirementButton.Tooltip"/>
                  </Supertip>
                  <Icon>
                    <bt:Image size="16" resid="Icon.16x16"/>
                    <bt:Image size="32" resid="Icon.32x32"/>
                    <bt:Image size="80" resid="Icon.80x80"/>
                  </Icon>
                  <Action xsi:type="ExecuteFunction">
                    <FunctionName>insertNewRequirement</FunctionName>
                  </Action>
                </Control>
              </Group>
            </OfficeTab>
          </ExtensionPoint>
        </DesktopFormFactor>
      </Host>
    </Hosts>
    <Resources>
      <bt:Images>
        <bt:Image id="Icon.16x16" DefaultValue="https://maximecharriere.github.io/WordReqInserter/assets/icon-16.png"/>
        <bt:Image id="Icon.32x32" DefaultValue="https://maximecharriere.github.io/WordReqInserter/assets/icon-32.png"/>
        <bt:Image id="Icon.80x80" DefaultValue="https://maximecharriere.github.io/WordReqInserter/assets/icon-80.png"/>
      </bt:Images>
      <bt:Urls>
        <bt:Url id="GetStarted.LearnMoreUrl" DefaultValue="https://go.microsoft.com/fwlink/?LinkId=276812"/>
        <bt:Url id="Commands.Url" DefaultValue="https://maximecharriere.github.io/WordReqInserter/commands.html"/>
      </bt:Urls>
      <bt:ShortStrings>
        <bt:String id="GetStarted.Title" DefaultValue="Commencez avec votre add-in de requis!"/>
        <bt:String id="ESPIGroup.Label" DefaultValue="ESPI"/>
        <bt:String id="InsertRequirementButton.Label" DefaultValue="Insert Requirement"/>
      </bt:ShortStrings>
      <bt:LongStrings>
        <bt:String id="GetStarted.Description" DefaultValue="Votre add-in de requis est chargÃ©. Allez dans l'onglet HOME et cliquez sur 'Insert Requirement' pour ajouter un nouveau requis."/>
        <bt:String id="InsertRequirementButton.Tooltip" DefaultValue="Click to insert a new requirement in the format [REQ_XXXX]."/>
      </bt:LongStrings>
    </Resources>
  </VersionOverrides>
</OfficeApp>